<%- include('header'); -%>
<%- include('navbar'); -%>
<%- include('sidenav'); -%>
<div id="page-content-wrapper"></div>
<h1 class="my-4">
    <%= videoTitle %>
</h1>
<h2>
    <%= videoDir %>
</h2>
<div class="well">
    <p> # of Videos
        <%= videoFiles.length %>
    </p>
</div>

<% for (k=0;k<videoFiles.length;k=k+4) { %>

<div class="row">
    <% for (i=k; i<k+4; i++) { %>
    <% if (videoFiles[i]) {el = videoFiles[i]; %>
    <div class="col">
        <p>
            <%= el.substr(0,15) %>
        </p>
        <video width="200" height="200" controls src="<%= el %>" type="video/mp4">
        </video>
    </div>
    <% }%>
    <%}%>
</div>
</div>
<% } %>
<%- include('footer'); -%>