<%- include('header'); -%>
<%- include('navbar'); -%>
<%- include('sidenav'); -%>
<div id="page-content-wrapper">
    <main role="main">
        <h1 class="my-4">
            <%= videoTitle %>
        </h1>

        <p class="lead"> # of Videos
            <%= videoFiles.length %>
        </p>
        <% for (k=0;k<videoFiles.length;k=k+4) { %>

        <div class="row">
            <% for (i=k; i<k+4; i++) { %>
            <% if (videoFiles[i]) {el = videoFiles[i]; %>
            <div class="col">

                <video width="200" height="200" controls src="<%= el %>" type="video/mp4">
                </video>
                <p>
                    <%= ((decodeURI(el).slice(0,-4)).substr(0,28)) %>
                </p>
            </div>
            <% }%>
            <%}%>
        </div>
    </main>
</div>
<% } %>
<%- include('footer'); -%>