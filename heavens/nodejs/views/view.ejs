<%- include('header'); -%>
<%- include('navbar'); -%>
<div>
<h1><%= videoTitle %></h1>

<p> # of Videos <%= videoFiles.length %> </p>
</div>
    <div class="video-row" >
       <% videoFiles.forEach ( function(file) { %> 
           <div class="vido-col">
               <video  controls src="<%= file %>" type="video/mp4"> 
               </video>
               <div><p> <%= ((decodeURI(file).slice(0,-4)).substr(0,35)) %></p></div>
           </div>

       <%}) %>
    </div>

<%- include('footer'); -%>
